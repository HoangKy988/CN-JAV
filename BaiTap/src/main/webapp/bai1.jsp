<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="vi">
<head>
  <meta charset="UTF-8">
  <title>Họ và Tên</title>
  <!-- Nhúng Bootstrap -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">

  <style>
    /* Tùy chỉnh thêm màu chữ và nền */
    .custom-text-color {
      color: #ffffff; /* màu chữ trắng */
    }

    .custom-bg-color {
      background-color: #007bff; /* màu nền xanh dương */
    }
  </style>
</head>
<body>

  <!-- Thẻ h1 với các class căn chỉnh -->
  <h1 class="text-center text-uppercase custom-text-color custom-bg-color p-3">
    Hoàng quang kỳ
  </h1>

</body>
</html>
